<h1>Create a new News</h1>
 
<%= form_with scope: :news, url: "/dailynews", local: true do |f| %>
  <p> 
    <%= f.label :headline %><br/> 
    <%= f.text_field :headline %>
  </p>
  <p>
    <%= f.label :information %><br/> 
    <%= f.text_area :information %> 
  </p>
  <p>
    <%= f.submit "create" %> 
  </p>
<% end %>